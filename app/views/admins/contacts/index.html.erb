<h2>問合せフォーム</h2>
<table class="table">
	<thead>
		<tr>
			<th>Email</th>
			<th>Day</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @contacts.each do |contact| %>
			<tr>
				<td><%= contact.email %></td>
				<td><%= contact.created_at.strftime("%Y%m月%d日 ") %></td>
				<td><%= link_to "返信", edit_admins_contact_path(contact) %></td>
			</tr>
		<% end %>
	</tbody>
</table>