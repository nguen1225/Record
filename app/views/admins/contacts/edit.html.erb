<h2>お問い合わせ返信</h2>
    <%= form_for @contact, url: admins_contact_path do |f| %>
        <h3>宛先</h3>
        <%= @contact.email %>
        <h3>お問い合わせ内容</h3>
        <%= @contact.message %>
        <h3>本文</h3>
        <%= f.text_area :reply, placeholder: "Sendcontents", size: "20x10", class: "form-control", required:true %><br>
        <%= f.submit "Send", class: "btn" %>
    <% end %>